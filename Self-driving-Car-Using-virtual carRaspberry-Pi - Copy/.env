# Emergency Services Configuration
# IMPORTANT: Never commit this file to version control!
# Add .env to .gitignore

# Gmail Configuration for Emergency Alerts
GMAIL_ADDRESS=your_email@gmail.com
GMAIL_PASSWORD=your_app_password

# Emergency Contact Information
EMERGENCY_EMAIL=emergency_contact@gmail.com
EMERGENCY_PHONE=+1234567890

# Vehicle Configuration
VEHICLE_MAX_SPEED=50
VEHICLE_MIN_DISTANCE_THRESHOLD=0.3

# Vision Configuration
YOLO_CONFIDENCE_THRESHOLD=0.5
LANE_DETECTION_INTERVAL=5

# Sensor Configuration
ULTRASONIC_TIMEOUT=1.0

# Safety Configuration
EMERGENCY_COOLDOWN_PERIOD=300
ACCELERATION_THRESHOLD=8.0
IMPACT_THRESHOLD=15.0
ROLLOVER_THRESHOLD=6.0

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/vehicle.log

